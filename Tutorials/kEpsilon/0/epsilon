FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 3.6e-7;

boundaryField
{
    top
    {
        type            slip;
    }

    bottom
    {
	    type		    zeroGradient;
    }
    

    atmosphere
    {
         type            inletOutlet;
	inletValue uniform 3.6e-7;
	value uniform 3.6e-7;
    }

    frontBack
    {
        type            empty;
    }
    
    inlet_half0
    {
	    type		    cyclic;
    }
    
 
    inlet_half1
    {
	    type		    cyclic;
    }
}

