FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet_half0
    {
        type            cyclic;
    }
    bottom
    {
        type            zeroGradient;
    }
    atmosphere
    {
        type            totalPressure;
        p0              uniform 0;
       
    }
    frontBack
    {
        type            empty;
    }
    inlet_half1
    {
        type            cyclic;
    }
}



